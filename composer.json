{
    "name": "whisker/application-insights",
    "description": "This project extends the Application Insights API surface to support PHP.",
    "type": "library",
    "keywords": [
        "log",
        "logging",
        "telemetry",
        "insights",
        "monitoring"
    ],
    "version": "0.5.0",
    "homepage": "https://github.com/Automated-Pet-Care-Products/ApplicationInsights-PHP",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "psr/http-message": "^2.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "ext-zlib": "*",
        "ext-mbstring": "*",
        "ext-curl": "*",
        "php-ds/php-ds": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3",
        "evert/phpdoc-md": "^0.2"
    },
    "autoload": {
        "psr-4": {
            "ApplicationInsights\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApplicationInsights\\Tests\\": "Tests/"
        }
    }
}
